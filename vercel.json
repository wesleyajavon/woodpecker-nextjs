{
  "functions": {
    "src/app/api/beats/[id]/files/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/beats/upload/route.ts": {
      "maxDuration": 300
    }
  },
  "headers": [
    {
      "source": "/api/beats/(.*)",
      "headers": [
        {
          "key": "Access-Control-Max-Age",
          "value": "86400"
        }
      ]
    }
  ]
}
